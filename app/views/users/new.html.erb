<h1>User#new</h1>
<h1>Sign Up</h1>
<% if flash[:errors] %>
    <% flash[:errors].each do |error| %>
   <li> <%= error %> </li>
    <% end %>
<% end %>

<%= form_for @user do |f|%>

  <%= f.label :username%><br>
  <%= f.text_field :username%><br>

  <%= f.label :password%><br>
  <%= f.password_field :password_digest %><br>

  <%= f.submit %>
<%end%>
